{
	"name": "PhotoCodeWeb",
	"version": "1.0.0",
  "license": "MIT",
	"scripts": {
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass public/snippets.css",
    "css-postcss": "postcss --use autoprefixer --output public/snippets.css public/snippets.css",
    "css-watch": "npm run css-build -- --watch",
		"dev": "nodemon app.js",
		"start": "npm run css-deploy && node app.js"
	},
	"dependencies": {
		"express": "4.17.1",
		"@google-cloud/vision": "1.11.0",
		"axios": "^0.19.2",
		"multer": "1.4.2"
	},
	"devDependencies": {
		"nodemon": "^2.0.4"
	}
}
